[
  {
    "name": "Smoke Test",
    "request": {
      "host": "https://api.publicapis.org",
      "path": "entries",
      "method": "get"
    },
    "exec": {
      "type": "api.smoke",
      "vus": "10",
      "iterations": "100",
      "insecure-skip-tls-verify": true,
      "include-system-env-vars": true,
      "max-redirects": 10,
      "no-connection-reuse": false,
      "no-vu-connection-reuse": false,
      "output": {
        "summary": true
      },
      "thresholds": [
        {
          "metric": "req_failed",
          "values": [
            "p(90)<200",
            "p(95)<250",
            "p(99)<300"
          ]
        }
      ],
      "response": {
        "headers": {
          "Header1": "Value",
          "Header2": "Value",
          "Header3": "Value"
        },
        "payload": "dqwdqd",
        "status": 400
      }
    }
  }
]