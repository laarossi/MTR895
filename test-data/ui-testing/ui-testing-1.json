[
  {
    "name" : "UI Test Case 1",
    "request" : {
      "host": "https://www.selenium.dev/documentation/webdriver/actions_api/mouse/",
      "path": "/",
      "method": "get"
    },
    "exec" : {
      "type" : "ui.simple",
      "webDriver" : "chrome",
      "options" : {
        "acceptInsecureCerts" : false,
        "pageLoadStrategy" : "eager",
        "timeouts" : 500
      },
      "check" : [

      ],
      "events" : [
        {
          "element" : "//span[text()='Downloads']",
          "selector" : "xPath",
          "event" : "click",
          "check-after" : [
            {
              "element" : "title",
              "value" : "Downloads | Selenium"
            }
          ],
          "check-before" : [
            {
              "element" : "title",
              "value" : "Mouse actions | Selenium"
            }
          ]
        }
      ]
    }
  }
]